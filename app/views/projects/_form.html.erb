<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>
  <%= f.input :title %>
  <%= f.input :short_description %>
  <%= f.input :technologies_used %>
  <% puts "Totals: " + Project.all.size.to_s %>
  <%= f.input :priority, input_html: { max: @max_priority, min: 1, value: 1}%>
  <%= f.input :web_page, as: :url %>
  <%= f.input :image, as: :file %>
  <%= f.button :submit %>
<% end %>
